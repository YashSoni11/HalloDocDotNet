@model HalloDoc_DAL.ProviderViewModels.Physicianshifts;
@using HalloDoc_DAL.ProviderViewModels;



<table class="table table-bordered">
    <thead>
        <tr style="background-color:#444444;color:white;">
            <th style="width:100%">STAFF</th>
            <th>12A</th>
            @for (int i = 1; i <= 11; i++)
            {
                <th>@(i)A</th>
            }
            <th>12P</th>
            @for (int i = 1; i <= 11; i++)
            {
                <th>@(i)P</th>
            }
        </tr>
        <tr>
            <th>Coverage</th>
            @for (int i = 0; i < 24; i++)
            {
                <th style="background-color:#edacea;"></th>
            }
        </tr>

    </thead>
    <tbody>
        @foreach (DayWiseShift dayWiseShift in Model.dayWiseShifts)
        {
            <tr>
                <th>Mr.Yash</th>
                @{
                    int start = dayWiseShift.startTime;
                    int end = dayWiseShift.endTime;
                }
                @for (int i = 0; i < 24; i++)
                {
                    if (i >= start && i <= end)
                    {
                        <th style="background-color:#edacea;"></th>
                    }
                    else
                    {

                        <th></th>
                    }
                }
            </tr>

        }

    </tbody>
</table>