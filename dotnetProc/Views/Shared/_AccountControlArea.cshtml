

@model HalloDoc_DAL.ProviderViewModels.CreateRole;


@{


    for(int i = 0; i < Model.AccessAreas.Count; i++)
    {
        <div class="form-check">
            <input asp-for="@Model.AccessAreas[i].IsAreaSelected" class="form-check-input" type="checkbox"  id="@Model.AccessAreas[i].AreaId">
            <label class="form-check-label" for="flexCheckDefault">
                @Model.AccessAreas[i].AreaName
            </label>
        </div>
        <input hidden asp-for="@Model.AccessAreas[i].AreaId" style="display:none;" />
    }


 }


